<%- include('cmsPartial/headCms') %>
<%- include('cmsPartial/cmsNav') %>

    <h2 class="text-center text-danger">List of Ruang Kepsek Category</h2>

    <!-- Data section -->
    <section>
        <div class="container mt-5">
            <div class="row">
                <div class="col-md-8 mx-auto">
                    <table class="table">
                        <thead>
                            <tr class="text-center">
                              <th scope="col">No</th>
                              <th scope="col">Category</th>
                              <th scope="col">CreatedAt</th>
                            </tr>
                          </thead>
                          <tbody >
                            <% data.forEach((el,i) => { %>
                                <tr class="text-center">
                                    <td><%= i + 1 %></td>
                                    <td><%= el.name%></td>
                                    <td><%= el.formatDate%></td>
                                </tr>
                            <% }) %>
                          </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
<%- include('cmsPartial/cmsFoot') %>